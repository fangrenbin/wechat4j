<?xml version="1.0" encoding="UTF-8"?>
<Root>
    <MessageHeader>
        <![CDATA[
            <ToUserName>${ToUserName}</ToUserName>
            <FromUserName>${FromUserName}</FromUserName>
            <CreateTime>${CreateTime}</CreateTime>
            <MsgType>${MsgType}</MsgType>
         ]]>
    </MessageHeader>
    <TextMessage>
        <![CDATA[
            <xml>
                ${MessageHeader}
                <Content>${Content}</Content>
            </xml>
        ]]>
    </TextMessage>
    <ImageMessage>
        <Common>
            <![CDATA[
                <xml>
                    ${MessageHeader}
                    <Image>${Image}</Image>
                </xml>
            ]]>
        </Common>
        <Image>
            <![CDATA[
                <MediaId>${MediaId}</MediaId>
            ]]>
        </Image>
    </ImageMessage>
    <VoiceMessage>
        <Common>
            <![CDATA[
                <xml>
                    ${MessageHeader}
                    <Voice>${Voice}</Voice>
                </xml>
            ]]>
        </Common>
        <Voice>
            <![CDATA[
                <MediaId>${MediaId}</MediaId>
            ]]>
        </Voice>
    </VoiceMessage>
    <VideoMessage>
        <Common>
            <![CDATA[
                <xml>
                    ${MessageHeader}
                    <Video>${Video}</Video>
                </xml>
            ]]>
        </Common>
        <Video>
            <![CDATA[
                <MediaId>${MediaId}</MediaId>
                <Title>${Title}</Title>
                <Description>${Description}</Description>
            ]]>
        </Video>
    </VideoMessage>
    <MusicMessage>
        <Common>
            <![CDATA[
                <xml>
                    ${MessageHeader}
                    <Music>${Music}</Music>
                </xml>
            ]]>
        </Common>
        <Music>
            <![CDATA[
                <Title>${Title}</Title>
                <Description>${Description}</Description>
                <MusicUrl>${MusicUrl}</MusicUrl>
                <HQMusicUrl>${HQMusicUrl}</HQMusicUrl>
                <ThumbMediaId>${ThumbMediaId}</ThumbMediaId>
            ]]>
        </Music>
    </MusicMessage>
    <NewsMessage>
        <Common>
            <![CDATA[
                <xml>
                    ${MessageHeader}
                    <ArticleCount>${ArticleCount}</ArticleCount>
                    <Articles>
                        ${Article}
                    </Articles>
                </xml>
            ]]>
        </Common>
        <Article>
            <![CDATA[
                <item>
                    <Title>${Title}</Title>
                    <Description>${Description}</Description>
                    <PicUrl>${Picurl}</PicUrl>
                    <Url>${Url}</Url>
                </item>
            ]]>
        </Article>
    </NewsMessage>
</Root>